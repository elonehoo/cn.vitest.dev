{
  "name": "docs",
  "packageManager": "pnpm@7.5.0",
  "private": true,
  "version": "0.21.0",
  "scripts": {
    "dev": "vitepress --port 3333 --open",
    "build": "pnpm run prefetch && vitepress build && esno .vitepress/scripts/build-pwa.ts",
    "build-no-prefetch": "vitepress build && esno .vitepress/scripts/build-pwa.ts",
    "serve": "vitepress serve",
    "preview-https": "pnpm run build && serve .vitepress/dist",
    "preview-https-no-prefetch": "pnpm run build-no-prefetch && serve .vitepress/dist",
    "prefetch": "esno .vitepress/scripts/fetch-avatars.ts"
  },
  "dependencies": {
    "@vueuse/core": "^9.1.0",
    "vue": "^3.2.37"
  },
  "devDependencies": {
    "@iconify-json/carbon": "^1.1.7",
    "@types/node": "^18.6.5",
    "@unocss/reset": "^0.45.5",
    "@vitejs/plugin-vue": "^3.0.1",
    "esno": "^0.16.3",
    "fast-glob": "^3.2.11",
    "https-localhost": "^4.7.1",
    "pnpm": "^7.9.0",
    "unocss": "^0.45.5",
    "unplugin-vue-components": "^0.22.2",
    "vite": "3.0.4",
    "vite-plugin-pwa": "0.12.3",
    "vitepress": "1.0.0-alpha.4",
    "workbox-window": "^6.5.4"
  }
}
